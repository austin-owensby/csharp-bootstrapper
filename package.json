{
	"name": "csharp-bootstrapper",
	"displayName": "C# Bootstrapper",
	"publisher": "austinowensby",
	"description": "Bootstraps a basic CRUD API based off of a C# model.",
	"icon": "resources/ajax.png",
	"version": "0.0.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/austin-owensby/csharp-bootstrapper"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"keyword":[
		"C#",
		"Bootstrapper",
		"CRUD",
		"Typescript",
		"API",
		"csharp",
		"ts"
	],
	"activationEvents": [
		"onCommand:csharp-bootstrapper.convertModel",
		"onCommand:csharp-bootstrapper.settings",
		"onCommand:csharp-bootstrapper.globalSettings"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "csharp-bootstrapper",
					"title": "C# Bootstrapper",
					"icon": "resources/icon.svg"
				}
			]
		},
		"views": {
			"csharp-bootstrapper": [
				{
					"id": "options",
					"name": "Options"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "options",
				"contents": "Welcome to C# Bootstrapper!\n Click the button below to navigate to the configuration settings for your project.\nFile paths are relative to the workspace folder\n[Workspace Settings](command:csharp-bootstrapper.settings)\n[Global Settings](command:csharp-bootstrapper.globalSettings)"
			}
		],
		"configuration": [
			{
				"title": "C# Bootstrapper",
				"properties": {
					"csharp-bootstrapper.frontendModelDirectory": {
						"type": "string",
						"default": "",
						"description": "The directory that frontend typescript models should be added."
					}
				}
			}
		],
		"commands": [
			{
				"command": "csharp-bootstrapper.convertModel",
				"title": "Convert to Typescript"
			},
			{
				"command": "csharp-bootstrapper.settings",
				"title": "C# Boostrapper Extension Workplace Settings"
			},
			{
				"command": "csharp-bootstrapper.gloablSettings",
				"title": "C# Boostrapper Extension Global Settings"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "csharp-bootstrapper.convertModel",
					"when": "editorLangId == csharp"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
